@{
    ViewBag.Title = "Open card bill";
}

@Html.Partial("_billList")
<script>
    $LAB.script(js("jqgrid", "moment", "datepicker")).wait(function () {
        bills_init({
            url: "/Cashier/GetOpenCardBills",
            colModel: [
                { name: 'BillNo', label: 'Bill No.' },
                { name: 'Name', label: 'Name' },
                { name: 'Phone', label: 'Phone' },
                { name: 'CardNo', label: '' },
                { name: 'CardType', label: '' },
                { name: 'Actual', label: 'Actual' },
                { name: 'Bonus', label: 'Bonus' },
                { name: 'EmployeeName', label: 'Employee name' },
                { name: 'CreateTime', label: 'Open time' },
                { name: 'Status', label: '' },
                { name: 'Operator', label: 'Operator' },
                { name: 'Note', label: 'Remark' },
            ],
        });
    });
</script>
